{
  "name": "awesomesearch-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/worker.js",
  "engines": {
    "node": ">=13.11"
  },
  "scripts": {
    "test": "jest",
    "start": "NODE_ENV=production ENV_PATH=.env.production nf start",
    "start:dev": "NODE_ENV=development ENV_PATH=.env.development nf start",
    "start:web": "NODE_ENV=development ENV_PATH=.env.development nodemon src/web/index.js",
    "test:unit": "jest src/tests/unit",
    "test:integration": "jest src/tests/integration",
    "coverage": "jest --coverage",
    "coverage:web": "npm run coverage && open coverage/lcov-report/index.html",
    "repo-graph-ddot": "depcruise --exclude \"^node_modules\" --output-type ddot src | dot -T svg > graph.ddot.svg",
    "repo-graph-dot": "depcruise --exclude \"^node_modules\" --output-type dot src | dot -T svg > graph.dot.svg",
    "count-lines": "find ./src -name '*.js' -not -path \"./src/tests*\" | xargs wc -l | sort -nr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartolomej/awesome-server.git"
  },
  "author": "Bartolomej Kozorog",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bartolomej/awesomesearch-api/issues"
  },
  "homepage": "https://github.com/bartolomej/awesomesearch-api#readme",
  "devDependencies": {
    "@jest/globals": "26.0.1",
    "jest": "25.1.0"
  },
  "dependencies": {
    "bull": "3.14.0",
    "bull-board": "0.7.0",
    "chalk": "3.0.0",
    "cheerio": "1.0.0-rc.3",
    "cloudinary": "1.21.0",
    "colors": "latest",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "envalid": "6.0.1",
    "express": "4.17.1",
    "express-handlebars": "4.0.4",
    "fetch-mock": "9.2.1",
    "flexsearch": "0.6.32",
    "foreman": "3.0.1",
    "ioredis": "4.16.3",
    "meow": "6.0.1",
    "morgan": "1.10.0",
    "mysql": "2.18.1",
    "node-fetch": "2.6.0",
    "normalize-url": "5.0.0",
    "puppeteer": "3.0.4",
    "reflect-metadata": "0.1.13",
    "remark-parse": "7.0.2",
    "rimraf": "3.0.2",
    "throng": "4.0.0",
    "typeorm": "0.2.25",
    "unified": "8.4.2",
    "unist-util-select": "3.0.1",
    "uuid": "latest",
    "winston": "3.2.1"
  }
}
